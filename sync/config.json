{
    "type": "flexible",
    "state": "enabled",
    "development_mode_enabled": true,
    "service_name": "letsbowl",
    "database_name": "letsbowlusersdb",
    "client_max_offline_days": 30,
    "is_recovery_mode_disabled": false,
    "permissions": {
        "rules": {},
        "defaultRoles": [
            {
                "name": "owner-read-write",
                "applyWhen": {},
                "read": {
                    "owner_id": "%%user.id"
                },
                "write": {
                    "owner_id": "%%user.id"
                }
            }
        ]
    },
    "queryable_fields_names": [
        "email",
        "password"
    ]
}
